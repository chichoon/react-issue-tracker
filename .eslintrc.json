{
  "extends": ["airbnb", "plugin:import/errors", "plugin:import/warnings", "airbnb/hooks", "react-app", "prettier"],
  "settings": {
    "import/extensions": [".js", ".jsx", ".ts", ".tsx"],
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    },
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  },
  "rules": {}
}
